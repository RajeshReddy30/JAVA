package com.xworkz.app.services;

import com.xworkz.app.Dto.ResortDto;

public class Resort {
	private ResortDto resortdto[]=new ResortDto[3];
	private int index;
	public String onSave(ResortDto dto) {
		if(this.index<resortdto.length) {
			resortdto[index]=dto;
			index++;
			
		
		}
			
		return "not saved";
	}

}
